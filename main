--- White List System

local allowedUserIds = {
	DirectMemoryAccess = 6156907237,
	XazmyBalenci = 3929630725,
	Caliber = 5671099212
}

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local isWhitelisted = false
for _, userId in pairs(allowedUserIds) do
	if LocalPlayer.UserId == userId then
		isWhitelisted = true
		break
	end
end

if not isWhitelisted then
	LocalPlayer:Kick("Trying to bypass our System. You are not listed.")
	return
end

--- Aimviewer System

local starterGui = game:GetService("StarterGui")

local function showNotification()
	starterGui:SetCore("SendNotification", {
		Title = "Falling Void",
		Text = "Go to settings to configure the Aim Viewer.",
		Duration = 5,
		Button1 = "Got It"
	})
end

local function toggleAimViewer()
	showNotification()

	while true do

		local player = game:GetService("Players").LocalPlayer
		local gui = player.PlayerGui.gui
		local aimViewerFrame = gui.Settings.ScrollingFrame.aimviewer

		aimViewerFrame.Visible = true

		task.wait(3)
	end
end

toggleAimViewer()

