-- Scripts:

local function KFWMQW_fake_script() -- Spectate.LocalScript 
	local script = Instance.new('LocalScript', Spectate)

	local textBox = script.Parent.Parent:WaitForChild("TextBox")
	local spectateButton = script.Parent.Parent:WaitForChild("Spectate")
	
	local isSpectating = false
	local spectateEnabledColor = Color3.fromRGB(0, 255, 0)
	local spectateDisabledColor = Color3.fromRGB(255, 0, 0)
	
	local player = game.Players.LocalPlayer
	local camera = game.Workspace.CurrentCamera
	
	local targetPlayer
	
	local function toggleSpectate()
		local username = textBox.Text
	
		if username and username ~= "" then
			if not isSpectating then
				for _, p in pairs(game.Players:GetPlayers()) do
					if p.Name:lower():sub(1, #username):lower() == username:lower() or p.DisplayName:lower():sub(1, #username):lower() == username:lower() then
						targetPlayer = p
						break
					end
				end
	
				if targetPlayer and targetPlayer.Character then
					local targetCharacter = targetPlayer.Character
					local humanoidRootPart = targetCharacter:WaitForChild("HumanoidRootPart", 5)
					if humanoidRootPart then
						camera.CameraType = Enum.CameraType.Scriptable
						camera.CameraSubject = targetCharacter.Humanoid
	
						isSpectating = true
	
						spectateButton.BackgroundColor3 = spectateEnabledColor
						spectateButton.TextColor3 = spectateEnabledColor
						spectateButton.TextStrokeTransparency = 0.5
						spectateButton.AutoButtonColor = false
					else
						warn("Target player does not have a HumanoidRootPart!")
					end
				else
					warn("Player not found or character not loaded: " .. username)
				end
			else
				-- Return to normal camera behavior
				camera.CameraType = Enum.CameraType.Custom
				camera.CameraSubject = player.Character.Humanoid
	
				spectateButton.BackgroundColor3 = spectateDisabledColor
				spectateButton.TextColor3 = spectateDisabledColor
				spectateButton.TextStrokeTransparency = 0
				spectateButton.AutoButtonColor = true
	
				isSpectating = false
				targetPlayer = nil
			end
		end
	end
	
	spectateButton.MouseButton1Click:Connect(function()
		toggleSpectate()
	end)
	
	game:GetService("RunService").Heartbeat:Connect(function()
		if isSpectating and targetPlayer and targetPlayer.Character then
			local humanoidRootPart = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				-- Adjust the camera position to stay slightly behind and above the player
				local cameraOffset = Vector3.new(0, 5, -10)  -- Camera is slightly above and behind
				local cameraPosition = humanoidRootPart.Position - humanoidRootPart.CFrame.LookVector * 10 + Vector3.new(0, 5, 0)
	
				-- Adjust the camera's CFrame to have a better view of the front
				camera.CFrame = CFrame.new(cameraPosition, humanoidRootPart.Position + humanoidRootPart.CFrame.LookVector * 3)
			end
		end
	end)
end
coroutine.wrap(KFWMQW_fake_script)()
local function XYOW_fake_script() -- Visuals.LocalScript 
	local script = Instance.new('LocalScript', Visuals)

	local Players = game:GetService("Players")
	local visualsButton = script.Parent
	local visualsEnabled = false
	local activeNametags = {}
	
	local function createNametag(player)
		local nametag = Instance.new("BillboardGui")
		nametag.Adornee = player.Character:WaitForChild("Head")
		nametag.Size = UDim2.new(0, 200, 0, 60)
		nametag.StudsOffset = Vector3.new(0, 1, 0)  -- Keep it close to the head, slightly below
		nametag.Name = player.Name .. "Nametag"
		nametag.AlwaysOnTop = true  -- Make the nametag visible through walls
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Parent = nametag
		textLabel.Size = UDim2.new(1, 0, 0.5, 0)
		textLabel.Position = UDim2.new(0, 0, 0, 0)  -- Position the text at the bottom of the nametag
		textLabel.Text = "[" .. player.DisplayName .. " (" .. player.Name .. ")]"
		textLabel.TextScaled = true
		textLabel.TextSize = 20
		textLabel.TextColor3 = Color3.new(255, 0, 0)
		textLabel.BackgroundTransparency = 1
		textLabel.Font = Enum.Font.SourceSans
		textLabel.TextStrokeTransparency = 0.5
		textLabel.TextStrokeColor3 = Color3.new(0, 0, 0)
	
		nametag.Parent = workspace.CurrentCamera
		return nametag
	end
	
	local function toggleVisuals()
		visualsEnabled = not visualsEnabled
	
		if visualsEnabled then
			for _, player in pairs(Players:GetPlayers()) do
				if player.Character then
					local nametag = createNametag(player)
					activeNametags[player.UserId] = nametag
				end
			end
			visualsButton.TextColor3 = Color3.fromRGB(0, 255, 0)
			visualsButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		else
			for _, nametag in pairs(activeNametags) do
				nametag:Destroy()
			end
			activeNametags = {}
			visualsButton.TextColor3 = Color3.fromRGB(255, 0, 0)
			visualsButton.BackgroundColor3 = Color3.fromRGB(97, 97, 97)
		end
	end
	
	local function onPlayerAdded(player)
		player.CharacterAdded:Connect(function()
			if visualsEnabled then
				createNametag(player)
			end
		end)
	
		player.CharacterRemoving:Connect(function()
			local nametag = workspace.CurrentCamera:FindFirstChild(player.Name .. "Nametag")
	
			if nametag then
				nametag:Destroy()
			end
		end)
	
		if visualsEnabled then
			createNametag(player)
		end
	end
	
	visualsButton.MouseButton1Click:Connect(toggleVisuals)
	
	Players.PlayerAdded:Connect(onPlayerAdded)
	
	for _, player in pairs(Players:GetPlayers()) do
		onPlayerAdded(player)
	end
end
coroutine.wrap(XYOW_fake_script)()
local function MCPT_fake_script() -- GoTo.LocalScript 
	local script = Instance.new('LocalScript', GoTo)

	local button = script.Parent
	local textBox = button.Parent:WaitForChild("TextBox")
	local spectateButton = button.Parent:WaitForChild("Spectate")
	local goToButton = button.Parent:WaitForChild("GoTo")
	
	local isSpectating = false
	local isTeleporting = false
	local spectateEnabledColor = Color3.fromRGB(0, 255, 0)
	local spectateDisabledColor = Color3.fromRGB(255, 0, 0)
	local goToEnabledColor = Color3.fromRGB(0, 255, 0)
	local goToDisabledColor = Color3.fromRGB(255, 0, 0)
	
	local player = game.Players.LocalPlayer
	local camera = game.Workspace.CurrentCamera
	
	local targetPlayer
	
	local function findTargetPlayer(username)
		for _, p in pairs(game.Players:GetPlayers()) do
			if p.Name:lower():sub(1, #username):lower() == username:lower() or p.DisplayName:lower():sub(1, #username):lower() == username:lower() then
				return p
			end
		end
		return nil
	end
	
	local function toggleSpectate()
		local username = textBox.Text
	
		if username and username ~= "" then
			targetPlayer = findTargetPlayer(username)
	
			if targetPlayer and targetPlayer.Character then
				local targetCharacter = targetPlayer.Character
				local humanoidRootPart = targetCharacter:WaitForChild("HumanoidRootPart", 5)
				if humanoidRootPart then
					camera.CameraType = Enum.CameraType.Scriptable
					camera.CameraSubject = targetCharacter.Humanoid
	
					isSpectating = true
	
					spectateButton.BackgroundColor3 = spectateEnabledColor
					spectateButton.TextColor3 = spectateEnabledColor
					spectateButton.TextStrokeTransparency = 0.5
					spectateButton.Text = "Spectate"
					spectateButton.AutoButtonColor = false
				end
			end
		end
	end
	
	local function toggleGoTo()
		local username = textBox.Text
	
		if username and username ~= "" then
			targetPlayer = findTargetPlayer(username)
	
			if targetPlayer and targetPlayer.Character then
				if not isTeleporting then
					player.Character:SetPrimaryPartCFrame(targetPlayer.Character.HumanoidRootPart.CFrame)
	
					goToButton.BackgroundColor3 = goToEnabledColor
					goToButton.TextColor3 = goToEnabledColor
					goToButton.TextStrokeTransparency = 0.5
					goToButton.Text = "Go To"
					goToButton.AutoButtonColor = false
	
					isTeleporting = true
				else
					goToButton.BackgroundColor3 = goToDisabledColor
					goToButton.TextColor3 = goToDisabledColor
					goToButton.TextStrokeTransparency = 0
					goToButton.Text = "Go To"
					goToButton.AutoButtonColor = true
	
					isTeleporting = false
				end
			end
		end
	end
	
	spectateButton.MouseButton1Click:Connect(function()
		toggleSpectate()
	end)
	
	goToButton.MouseButton1Click:Connect(function()
		toggleGoTo()
	end)
	
	game:GetService("RunService").Heartbeat:Connect(function()
		if isSpectating and targetPlayer and targetPlayer.Character then
			local humanoidRootPart = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				local cameraOffset = Vector3.new(0, 5, -10)
				local cameraPosition = humanoidRootPart.Position - humanoidRootPart.CFrame.LookVector * 10 + Vector3.new(0, 5, 0)
	
				camera.CFrame = CFrame.new(cameraPosition, humanoidRootPart.Position + humanoidRootPart.CFrame.LookVector * 3)
			end
		end
	end)
end
coroutine.wrap(MCPT_fake_script)()
local function OJCFWI_fake_script() -- Vanish.LocalScript 
	local script = Instance.new('LocalScript', Vanish)

	local button = script.Parent
	local isVanished = false
	local originalPosition = nil
	local vanishPosition = Vector3.new(0, 33617.301, 0)
	
	local player = game.Players.LocalPlayer
	
	button.Text = "Vanish"
	button.TextColor3 = Color3.new(1, 0, 0)
	
	local function toggleVanish()
		isVanished = not isVanished
	
		if isVanished then
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				originalPosition = player.Character.HumanoidRootPart.Position
				player.Character.HumanoidRootPart.CFrame = CFrame.new(vanishPosition)
			end
			button.Text = "Vanish"
			button.TextColor3 = Color3.new(0, 1, 0)
		else
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and originalPosition then
				player.Character.HumanoidRootPart.CFrame = CFrame.new(originalPosition)
			end
			button.Text = "Vanish"
			button.TextColor3 = Color3.new(1, 0, 0)
		end
	end
	
	button.MouseButton1Click:Connect(toggleVanish)
end
coroutine.wrap(OJCFWI_fake_script)()
local function AZDK_fake_script() -- UI.DragScript 
	local script = Instance.new('LocalScript', UI)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(AZDK_fake_script)()
local function ZIAKK_fake_script() -- UI.Keybind 
	local script = Instance.new('LocalScript', UI)

	local UserInputService = game:GetService("UserInputService")
	
	function onKeyPress(inputObject, gameProcessedEvent)
		if not gameProcessedEvent then
			if inputObject.KeyCode == Enum.KeyCode.P then 
				if script.Parent.Parent.UI.Visible == false then
					script.Parent.Parent.UI.Visible = true
				else
					script.Parent.Parent.UI.Visible = false
				end
			end
		end
	end
	
	UserInputService.InputBegan:connect(onKeyPress)
end
coroutine.wrap(ZIAKK_fake_script)()
local function ASSSAK_fake_script() -- UI.ScreenPosition 
	local script = Instance.new('LocalScript', UI)

	local player = game.Players.LocalPlayer
	local gui = player.PlayerGui:WaitForChild("Falling Void")
	local uiFrame = gui:WaitForChild("UI")
	
	local function centerUI()
		uiFrame.AnchorPoint = Vector2.new(0.5, 0.5)
		uiFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
	end
	
	centerUI()
	
	game:GetService("GuiService").Changed:Connect(function()
		centerUI()
	end)
end
coroutine.wrap(ASSSAK_fake_script)()
